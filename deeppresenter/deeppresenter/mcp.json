[
    {
        "name": "deeppresenter",
        "description": "DeepPresenter Tools",
        "command": "uv",
        "args": [
            "run",
            "deeppresenter/tools/server.py",
            "$WORKSPACE"
        ],
        "env": {
            "FIRECRAWL_API_KEY": "your_key",
            "MINERU_API_KEY": "your_key",
            "MIN_IMAGE_SIZE": "921600",
            "TAVILY_API_KEY": "your_key"
        }
    },
    {
        "name": "pptagent",
        "description": "https://github.com/icip-cas/PPTAgent",
        "command": "uv",
        "args": [
            "run",
            "pptagent-mcp"
        ],
        "env": {
            "PPTAGENT_MODEL": "glm-4.6",
            "PPTAGENT_API_KEY": "your_key",
            "PPTAGENT_API_BASE": "https://open.bigmodel.cn/api/paas/v4/"
        }
    },
    {
        "name": "desktop_commander",
        "description": "https://github.com/wonderwhy-er/DesktopCommanderMCP",
        "command": "docker",
        "args": [
            "run",
            "--security-opt",
            "seccomp=unconfined",
            "--init",
            "--name",
            "$WORKSPACE_ID",
            "-i",
            "--rm",
            "-v",
            "$WORKSPACE:$WORKSPACE",
            "-w",
            "$WORKSPACE",
            "desktop-commander-deeppresenter"
        ]
    }
]